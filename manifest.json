    {
      "name": "Search Jobs",
      "short_name": "Search",
      "description": "Search for jobs in Ethiopia",
      "url": "/?shortcut=search",
      "icons": [
        {
          "src": "/assets/images/shortcuts/search.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Free Courses",
      "short_name": "Courses",
      "description": "Access free career courses",
      "url": "/?shortcut=courses",
      "icons": [
        {
          "src": "/assets/images/shortcuts/courses.png",
          "sizes": "96x96",
