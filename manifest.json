        }
      ]
    },
    {
      "name": "Post a Job",
      "short_name": "Post Job",
      "description": "Post job openings for employers",
      "url": "/?shortcut=post-job",
      "icons": [
        {
          "src": "/assets/images/shortcuts/post-job.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Applications",
      "short_name": "Applications",
      "description": "View your job applications",
      "url": "/?shortcut=applications",
      "icons": [
        {
          "src": "/assets/images/shortcuts/applications.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "auto"
  },
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "protocol_handlers": [
    {
      "protocol": "web+zewedjobs",
      "url": "/?protocol=%s"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "iarc_rating_id": "e84b072d-4b18-4f6a-8b58-3a5a8c7d9e1f",
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/upload-resume",
      "accept": {
        "application/pdf": [".pdf"],
        "application/msword": [".doc"],
        "application/vnd.openxmlformats-officedocument.wordprocessingml.document": [".docx"]
      },
      "icons": [
        {
          "src": "/assets/images/file-handler.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "name": "Upload Resume",
      "launch_type": "single-client"
    }
  ],
  "capture_links": "new-client",
  "serviceworker": {
    "src": "/sw.js",
    "scope": "/",
    "use_cache": true
  }
}
